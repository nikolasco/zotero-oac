    <!-- LONG LOCATOR FORMS -->
    <term name="page">
      <single>頁</single>
      <multiple>頁</multiple>
    </term>
    <term name="paragraph">
      <single>段落</single>
      <multiple>段落</multiple>
    </term>
    <term name="line">
      <single>行</single>
      <multiple>行</multiple>
    </term>
    <term name="volume">卷次</term>
    <term name="issue">期號</term>
    

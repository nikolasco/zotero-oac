    <term name="editor" form="verb">編者是</term>
    <term name="translator" form="verb">譯者是</term>
    
    <!-- SHORT VERB ROLE FORMS -->
    <term name="editor" form="verb-short">編</term>
    <term name="translator" form="verb-short">譯</term>
    

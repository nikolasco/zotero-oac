    <term name="page" form="short">
      <single>頁</single>
      <multiple>頁</multiple>
    </term>
    <term name="paragraph" form="short">
      <single>¶</single>
      <multiple>¶¶</multiple>
    </term>
    <term name="line" form="short">
      <single>行</single>
      <multiple>行</multiple>
    </term>
    <term name="volume" form="short">卷</term>
    <term name="issue" form="short">期</term>
    
